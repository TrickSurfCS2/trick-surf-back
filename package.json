{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "back",
  "description": "back",
  "version": "1.0.0",
  "source": "src/index.ts",
  "main": "./dist/index.js",
  "author": {
    "name": "Ivan Kornilov",
    "email": "ivan.kornilov@nordclan.com",
    "url": "https://t.me/xsolncu"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "backend"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=20.x",
    "pnpm": ">=8.x",
    "bun": ">=1.x"
  },
  "scripts": {
    "dev": "bun --bun run src/index.ts",
    "node:dev": "tsnd --respawn --pretty --transpile-only -r tsconfig-paths/register src/index.ts",
    "start": "bun run prisma:generate && bun dist/index.js",
    "node:start": "npm run prisma:generate && node dist/index.js",
    "build": "npm run prisma:generate && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.json",
    "------------------------------------------------------------------------------------------------------------": "",
    "lint": "eslint --fix \"./**/*.{ts,js}\"",
    "format": "prettier -w \"./**/*.{ts,json,scss,md}\"",
    "-------------------------------------------------------------------------------------------------------------": "",
    "prisma:migrate": "prisma migrate dev --skip-seed",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "prisma:seed": "prisma db seed",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "--------------------------------------------------------------------------------------------------------------": "",
    "prepare": "husky install .husky",
    "postinstall": "npm run prisma:generate"
  },
  "dependencies": {
    "@faker-js/faker": "^8.1.0",
    "@prisma/client": "^5.3.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "prom-client": "^14.2.0",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.6.4",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "crypto": "^1.0.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.3.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "seed": "bun x ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "lint-staged": {
    "*.{ts}": "eslint --fix",
    "*": "prettier -w -u"
  }
}
