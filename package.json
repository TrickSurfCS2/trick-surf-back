{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "trick-surf-back",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "bun@1.1.15",
  "description": "trick-surf-back",
  "license": "MIT",
  "keywords": [
    "backend"
  ],
  "main": "./dist/index.cjs",
  "files": [
    "README.md",
    "dist"
  ],
  "engines": {
    "node": ">=20.x",
    "bun": ">=1.x"
  },
  "scripts": {
    "dev": "tsup src/index.ts --format esm --watch --onSuccess 'node dist/index.js'",
    "build": "npm run prisma:generate && tsup --config build/build-server.ts",
    "start": "node dist/index.cjs",
    "------------------------------------------------------------------------------------------------------------": "",
    "lint": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "-------------------------------------------------------------------------------------------------------------": "",
    "test": "vitest --run --reporter verbose",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "--------------------------------------------------------------------------------------------------------------": "",
    "prisma:migrate": "prisma migrate dev --skip-seed",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "prisma:seed": "prisma db seed",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "---------------------------------------------------------------------------------------------------------------": "",
    "prepare": "husky install .husky",
    "postinstall": "npm run prisma:generate"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "@prisma/client": "^5.7.1",
    "@trpc/server": "^10.45.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "prom-client": "^15.1.0",
    "socket.io": "^4.7.3",
    "uuid": "^9.0.1",
    "prisma": "^5.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.1",
    "@tsconfig/node20": "^20.1.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.7",
    "@types/uuid": "^9.0.7",
    "@vitest/coverage-v8": "^1.1.3",
    "crypto": "^1.0.1",
    "eslint": "^9.5.0",
    "eslint-plugin-format": "^0.1.2",
    "esno": "^4.0.0",
    "husky": "^8.0.3",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "1.1.3"
  },
  "prisma": {
    "seed": "esno prisma/seed.ts"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
