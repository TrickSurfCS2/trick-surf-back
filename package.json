{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "back",
  "type": "module",
  "version": "1.0.0",
  "description": "back",
  "source": "src/index.ts",
  "author": {
    "name": "Ivan Kornilov",
    "email": "ivan.kornilov@nordclan.com",
    "url": "https://t.me/xsolncu"
  },
  "license": "MIT",
  "keywords": [
    "backend"
  ],
  "main": "./dist/index.js",
  "files": [
    "README.md",
    "dist"
  ],
  "engines": {
    "node": ">=20.x",
    "bun": ">=1.x"
  },
  "scripts": {
    "dev": "esno src/index.ts --watch",
    "start": "npm run prisma:generate && node dist/index.js",
    "node:dev": "tsnd --respawn --pretty --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "npm run prisma:generate && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.json",
    "------------------------------------------------------------------------------------------------------------": "",
    "lint": "eslint . --fix",
    "-------------------------------------------------------------------------------------------------------------": "",
    "test": "vitest --run --reporter verbose",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "--------------------------------------------------------------------------------------------------------------": "",
    "prisma:migrate": "prisma migrate dev --skip-seed",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "prisma:seed": "prisma db seed",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "---------------------------------------------------------------------------------------------------------------": "",
    "prepare": "husky install .husky",
    "postinstall": "npm run prisma:generate"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "@prisma/client": "^5.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "prom-client": "^15.0.0",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.1.1",
    "@tsconfig/node20": "^20.1.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "crypto": "^1.0.1",
    "eslint": "^8.54.0",
    "esno": "^4.0.0",
    "husky": "^8.0.3",
    "prisma": "^5.6.0",
    "typescript": "^5.3.2",
    "vitest": "0.34.6"
  },
  "prisma": {
    "seed": "esno prisma/seed.ts"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
